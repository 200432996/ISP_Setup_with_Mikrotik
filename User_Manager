/ip neighbor discovery-settings
set discover-interface-list=all
/interface detect-internet
set detect-interface-list=all
/interface pppoe-server server
add disabled=no interface=ether1-LAN one-session-per-host=yes service-name=\
    service1
add disabled=no interface=PON-1 one-session-per-host=yes service-name=service2
add disabled=no interface=PON-2 one-session-per-host=yes service-name=service3
add disabled=no interface=PON-3 one-session-per-host=yes service-name=service4
add disabled=no interface=PON-4 one-session-per-host=yes service-name=service5
add disabled=no interface=ether2-Router one-session-per-host=yes service-name=\
    service6
/interface pptp-server server
set enabled=yes
/ip address
add address=10.255.10.2/30 interface=IIG_Services network=10.255.10.0
add address=172.16.209.9/30 comment=OLT interface=ether1-LAN network=\
    172.16.209.8
/ip dns
set servers=8.8.8.8,1.1.1.1
/ip route
add check-gateway=ping distance=1 gateway=10.255.10.1
/ip service
set telnet disabled=yes
set ftp disabled=yes
set www port=xxxx
set ssh disabled=yes
set api port=xxxx
set winbox port=xxxx
set api-ssl disabled=yes
