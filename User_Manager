/interface bridge
add name=loopback
/interface ethernet
set [ find default-name=ether1 ] name=ether1-LAN
set [ find default-name=ether2 ] name=ether2-Router
set [ find default-name=sfp-sfpplus1 ] advertise=\
    10M-half,10M-full,100M-half,100M-full,1000M-half,1000M-full,10000M-full \
    auto-negotiation=no name=sfp-sfpplus1-DOL-WAN speed=1Gbps
/interface vlan
add interface=sfp-sfpplus1-DOL-WAN name=IIG_Services vlan-id=2010
add interface=ether1-LAN name=PON-1 vlan-id=501
add interface=ether1-LAN name=PON-2 vlan-id=502
add interface=ether1-LAN name=PON-3 vlan-id=503
add interface=ether1-LAN name=PON-4 vlan-id=504
/interface ethernet switch port
set 0 default-vlan-id=0
set 1 default-vlan-id=0
set 2 default-vlan-id=0
set 3 default-vlan-id=0
set 4 default-vlan-id=0
set 5 default-vlan-id=0
set 6 default-vlan-id=0
set 7 default-vlan-id=0
set 8 default-vlan-id=0
set 9 default-vlan-id=0
set 10 default-vlan-id=0
set 11 default-vlan-id=0
/ip pool
add name=100Mbps ranges=10.101.10.2-10.101.10.254
add name=5Mbps ranges=10.101.50.2-10.101.50.254
add name=50Mbps ranges=10.101.60.2-10.101.60.254
add name=70Mbps ranges=10.101.70.2-10.101.70.253
add name=80Mbps ranges=10.101.80.2-10.101.80.254
add name=10Mbps ranges=10.101.250.2-10.101.250.254
add name=15Mbps ranges=10.101.150.2-10.101.150.254
add name=20Mbps ranges=10.101.200.2-10.101.200.254
add name=30Mbps ranges=10.101.201.2-10.101.201.254
add name=60Mbps ranges=10.101.202.2-10.101.202.254
/ppp profile
add dns-server=103.179.14.14,8.8.8.8 local-address=10.101.10.1 name=100Mbps \
    on-up="foreach dev in=[ppp active print detail as-value where name=\"\$user\
    \" ] do={\r\
    \n \t/log info (\"PPPLOG \$user \" . (\$dev->\"caller-id\") . \" \" . (\$de\
    v->\"address\"));\r\
    \n};\r\
    \n" only-one=yes remote-address=100Mbps
add dns-server=103.179.14.14,8.8.8.8 local-address=10.101.50.1 name=5Mbps \
    on-up="foreach dev in=[ppp active print detail as-value where name=\"\$user\
    \" ] do={\r\
    \n \t/log info (\"PPPLOG \$user \" . (\$dev->\"caller-id\") . \" \" . (\$de\
    v->\"address\"));\r\
    \n};\r\
    \n" only-one=yes remote-address=5Mbps use-compression=no
